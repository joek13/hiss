{
  "name": "playground",
  "version": "0.1.0",
  "bin": {
    "playground": "bin/playground.js"
  },
  "scripts": {
    "lint": "eslint ./lib/ --ext .ts,.tsx",
    "lint:fix": "eslint ./lib/ --ext .ts,.tsx --fix",
    "format": "prettier --write ./lib/",
    "format:check": "prettier --check ./lib/",
    "bundle": "./bin/generate-bundle.bash",
    "prebuild": "npm run lint && npm run format:check && npm run bundle",
    "build": "tsc",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1021.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.205.0",
    "constructs": "^10.0.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.37.0"
  }
}
